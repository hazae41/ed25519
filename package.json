{
  "type": "module",
  "name": "@hazae41/ed25519",
  "version": "1.0.0",
  "description": "Ed25519 adapter for WebAssembly and JS implementations",
  "homepage": "https://github.com/hazae41/ed25519",
  "repository": "github:hazae41/ed25519",
  "author": "hazae41",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/esm",
    "./dist/cjs",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "node ./dist/test/index.test.mjs",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@hazae41/berith": "^1.1.40",
    "@hazae41/phobos": "^1.0.10",
    "@noble/curves": "^1.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.4.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-node-externals": "^6.1.1",
    "typescript": "^5.1.6"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "ed25519",
    "crypto",
    "curve25519",
    "cryptography",
    "adapter",
    "typescript",
    "esmodules"
  ],
  "dependencies": {
    "@hazae41/result": "^1.0.52"
  }
}
